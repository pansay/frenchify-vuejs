{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": [
    "jasmine"
  ],
  "env": {
    "jasmine": true
  },
  "rules": {
    "brace-style": ["error", "stroustrup"],
    "max-len": ["warn", {"code": 160}],
    "prefer-template": ["warn"],
    "no-param-reassign": ["warn"],
    "no-else-return": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0
  }
}
